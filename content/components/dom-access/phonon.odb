from vnfb.components.DOMAccessor import DOMAccessor as base

class Accessor(base):


    def __init__(self):
        super(Accessor, self).__init__('phonon-accessor')
        return


    def getDispersions(self, atomicstructure):
        db = self.db
        return atomicstructure.getReferences(db, PhononDispersionTable, 'matter')


    def getDOSes(self, atomicstructure):
        db = self.db
        return atomicstructure.getReferences(db, PhononDOSTable, 'matter')



from vnfb.dom.material_simulations.PhononDispersion import PhononDispersion, PhononDispersionTable
from vnfb.dom.material_simulations.PhononDOS import PhononDOS, PhononDOSTable


def accessor():
    return Accessor()


